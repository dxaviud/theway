{
  "name": "theway",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "watch": "rm -rf dist && pnpm tsc -w",
    "dev": "nodemon dist/index.js",
    "build": "pnpm tsc",
    "start": "node dist/index.js",
    "migration:gen": "pnpm build && pnpm typeorm migration:generate ./src/migration/migration -d ./dist/data-source.js",
    "migration:run": "pnpm build && pnpm typeorm migration:run -d ./dist/data-source.js",
    "clean": "rm -rf dist & rm -rf node_modules && pnpm i",
    "schema:drop": "pnpm build && pnpm typeorm schema:drop -d ./dist/data-source.js",
    "envgen": "npx gen-env-types .env -o src/env.d.ts -e ."
  },
  "keywords": [],
  "author": "David Xu",
  "license": "MIT",
  "devDependencies": {
    "@types/connect-redis": "^0.0.19",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.11.7",
    "@types/nodemailer": "^6.4.6",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.10",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "argon2": "^0.30.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.3",
    "graphql": "^15.3.0",
    "ioredis": "^5.2.4",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "validator": "^13.7.0"
  }
}
