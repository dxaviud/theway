  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.7",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  }
}
